<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.fxml.FXMLLoader?>
<?import javafx.scene.control.Label?>

<VBox alignment="CENTER" spacing="10.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.pokemonGame.Controllers.InitController">

      <children>
            <Label text="Bienvenido al juego" styleClass="Start__Page__Title"/>
            <Label text="Para comenzar favor de registrar minimo dos jugadores" styleClass="Start__Page__Description"/>
            <Button text="Registro de Jugadores" onAction ="#goToPlayerRegister"  styleClass="Start__Page__Buttons"/>
            <Button text="Iniciar juego" onAction ="#goToGame" fx:id="StartGameButton" styleClass="Start__Page__Buttons"/>
      </children>

</VBox>